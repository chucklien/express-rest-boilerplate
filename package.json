{
  "name": "express-rest-boilerplate",
  "version": "0.0.0",
  "description": "express rest boilerplate",
  "repository": "https://github.com/chucklien/express-rest-boilerplate",
  "author": "Chuck Lien <chuck.lien@gmail.com>",
  "private": true,
  "scripts": {
    "start": "nodemon -r dotenv/config ./bin/www",
    "serve": "DEBUG=faciallab-api node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-node": "^7.0.1",
    "nodemon": "^1.18.4"
  },
  "bin": "./bin/www",
  "engines": {
    "node": ">=10.9.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "next"
        }
      ]
    }
  }
}
